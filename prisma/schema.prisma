generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Good {
  id          String @id @default(auto()) @map("_id") @db.ObjectId 
  description String
  price       Int
  count       Int 
} 

model users {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  email       String
  password    String
} 